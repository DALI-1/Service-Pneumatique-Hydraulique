"use school"=" switch to the database school, if it doesnt exist, create it"
"db.createCollection('students')"="Creates a collection"
"db.dropDatabase()"="Drops a database"
"db.students.insertOne({name:'spongbob',age:'30',GPA:3.2})"="Insert a Document into a collection"
"db.students.find()"="Get all the documents from the collection"
"db.students.insertMany([{name:'patrick',age:38,GPA:1.5},{name:'Sandy',age:27,gpa:4.0},{name:'garry',age:22,GPA:4}])"="Insert more than one data"
"db.students.insertOne({name:'Larry',age:32,gpa:2.8,fullTime:false,registerDate: new Date(),gradutionDate:null,courses:['Biology','Chemistry','Calculas'],address:{street:'123',city:'bikinibottom'})"="All mongodb data types"

" db.students.find().sort({name:1})"="Sort by name, (1 alpha betique order, -1 reverse alphabetique"
"db.students.find().limit(1)"="Limits how many data is returned"
" db.students.find().sort({gpa:-1}).limit(1)"="Gets the one with the highest GPA"
"db.students.find({name:'spongbob'})"="Get students with the name spongbobg"
"db.students.find({},{name:true,})"="first parameter is the filter, the projection is to show which rows"
"db.students.updateOne({_id:ObjectId('64fee87acdb4899927794847')},{$set:{fullTime:true}})"="Updated a document by id, ($set means to add a field,$unset removes it)"
"db.students.updateMany({},{$set:{fullTime:false}})"="giving everyone a fulltime attribute"
"db.students.updateOne({name:'Sandy'},{$unset:{fullTime:''}})"="removed sandy's fulltime"
"db.students.updateMany({fullTime:{$exists:false}},{$set:{fullTime:true}})"="Set all of the documents that doesnt have a fulltime variable to true"
"db.students.deleteOne({name:'larry'})"="Delete a document"
"db.students.deleteMany({registerDate:{$exists:false}})"="Delete documents where they don't have a RegisterDate"

"db.students.find({name:{$ne:'spongbob'}})"="Find anyone that doesn't have a name equal to spongbob"
" db.students.find({age:{$lt:20}})"="Find anyone where he has age less equal than 20"
"db.students.find({gpa:{$gte:3,$lte:4}})"="find anyone with a gpa between 3 and 4"
"db.students.find({name:{$in:['spongbobg','Sandy']}})"="Find users where their name is either spongbob or sandy"
"db.students.find({$and:[{fullTime:true},{age:{$lte:22}}]})" ="give me a full time student and age less than 22"
"db.students.find({name:'Larry'}).explain('executionStats')"="Get users that has the name of garry and with explain we get more details about the query"
"db.students.createIndex({name:1})"="Create index for collumn name to improve get operation but slows create"
" db.students.getIndexes()"="Get the indexes of a collection"
"db.students.dropIndex('name_1')"="Drop a collection index"
"show collections"="See collections"
